<?xml version="1.0" encoding='UTF-8'?>
<odoo>
    <data noupdate="1">
        <!-- sequence master data pasien -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Sequence Pasien</field>
            <field name="code">hospital.patient.sequence</field>
            <field name="prefix">HP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- sequence appointment -->
        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Sequence Appointment</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">HA</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>